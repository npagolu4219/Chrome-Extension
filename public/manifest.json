  {
    "manifest_version": 3,
    "name": "muffins",
    "version": "1.0",
    "permissions": ["activeTab"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "run_at": "document_end"
      }
    ],
    "background": {
      "service_worker": "background.js"
    },
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": "favicon.ico"
    }
  }